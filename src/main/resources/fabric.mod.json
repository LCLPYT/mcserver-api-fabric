{
  "schemaVersion": 1,
  "id": "mcserver-api",
  "version": "${version}",
  "name": "MCServer Fabric Implementation",
  "description": "A Java library to manage Minecraft server backend interaction with LCLPNetwork. For Fabric.",
  "authors": [
    "LCLP"
  ],
  "contact": {
    "homepage": "https://github.com/LCLPYT/mcserver-api-fabric",
    "sources": "https://github.com/LCLPYT/mcserver-api-fabric",
    "issues": "https://github.com/LCLPYT/mcserver-api-fabric/issues"
  },
  "license": "MIT",
  "icon": "assets/mcserver-api/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      "work.lclpnet.serverimpl.kibu.MCServerFabricMod"
    ],
    "kibu-language": [
      "work.lclpnet.serverimpl.kibu.service.MCServerLanguageProvider"
    ]
  },
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.21",
    "java": ">=21",
    "fabric-api": "*",
    "kibu": "*"
